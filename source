			UICorner_14.CornerRadius = UDim.new(0, 12)
			UICorner_14.Parent = Ting


			local toggl = false
			local debounce = false




			TextButton.MouseButton1Click:Connect(function()
				if debounce == false then
					debounce = true
					if toggl == false then
						Ting:TweenPosition(UDim2.new(0.65, 0, 0, 0), Enum.EasingDirection.InOut, Enum.EasingStyle.Sine, 0.75)
						wait(0.8)
						toggl = true
						debounce = false
					elseif toggl == true then
						Ting:TweenPosition(UDim2.new(0, 0, 0, 0), Enum.EasingDirection.InOut, Enum.EasingStyle.Sine, 0.75)
						wait(0.8)
						toggl = false
						debounce = false
					end
				end
			end)
		end


