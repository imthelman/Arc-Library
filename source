	Subtitle.TextXAlignment = Enum.TextXAlignment.Left

	Minimize.MouseButton1Click:Connect(function()
		TopBar.Visible = false
	end)
	
	local uhuh = true
	Subtract.MouseButton1Click:Connect(function()
		if uhuh == true then
			BG.Visible = false
			uhuh = false
		elseif uhuh == false then
			BG.Visible = true
			uhuh = true
		end
	end)

	-- Functions

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));

	local gui = TopBar

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function Lerp(a, b, m)
		return a + (b - a) * m
	end;

	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (75); -- // The speed of the UI darg.
	local function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;

		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	runService.Heartbeat:Connect(Update)

	-- Make Tabs
	_G.InsideLib = {}
	
	function _G.InsideLib:CreateTab(tabparams)
		
		tabparams = tabparams or {}
		local name = tabparams.Name or 'Tab'

		local TabButton = Instance.new("TextButton")
		local UICornerTB = Instance.new("UICorner")
		local Tab = Instance.new("ScrollingFrame")
		
		local UIListLayout = Instance.new("UIListLayout")
		
		UIListLayout.Parent = SideBar
		UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.Padding = UDim.new(0,5)

		TabButton.Name = name.." Button"
		TabButton.Parent = SideBar
		TabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabButton.BackgroundTransparency = 0.900
		TabButton.BorderSizePixel = 0
		TabButton.Size = UDim2.new(0, 87, 0, 30)
		TabButton.Font = Enum.Font.Gotham
		TabButton.Text = name
		TabButton.TextColor3 = Color3.fromRGB(229, 229, 229)
		TabButton.TextSize = 14.000

		UICornerTB.CornerRadius = UDim.new(0, 25)
		UICornerTB.Name = "UICornerTB"
		UICornerTB.Parent = TabButton

		Tab.Name = name..' Container'
		Tab.Parent = Tabs
		Tab.Active = true
		Tab.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		Tab.BackgroundTransparency = 1.000
		Tab.BorderSizePixel = 0
		Tab.Position = UDim2.new(0.0507462695, 0, 0.0656565651, 0)
		Tab.Size = UDim2.new(0, 298, 0, 170)
		Tab.CanvasSize = UDim2.new(0, 0, 5.5, 0)
		Tab.ScrollBarThickness = 7
		Tab.Visible = false

		UIListLayout.Parent = Tab
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.Padding = UDim.new(0, 25)


		-- Tab Button
		TabButton.MouseButton1Click:Connect(function()
			for i,v in next, Tabs:GetChildren() do
				if not v:IsA('UICorner') then
					if not v:IsA('UIListLayout') then
						v.Visible = false
					end
				end
			end
			for i,v in next, SideBar:GetChildren() do
				if not v:IsA('UICorner') then
					if not v:IsA('UIListLayout') then
						v.BackgroundTransparency = 1
					end
				end
			end
			TabButton.BackgroundTransparency = 0.9
			Tab.Visible = true
		end)

		_G.InsideTab = {}

